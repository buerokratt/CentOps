{
  "age": [
    {
      "regex": "^(?:[1-9][0-9]{2,}|1000+)$",
      "errorMessage": "Value must be less than 100"
    },
    {
      "regex": "^(?:0*(?:1[0-7]|0?[0-9]))$",
      "errorMessage": "Value must be bigger than 18"
    }
  ],
  "name": [
    {
      "regex": "^[a-zA-Z0-9]{0,3}$",
      "errorMessage": "Must be more than 3 letters"
    },
    {
      "regex": "[0-9]",
      "errorMessage": "Number not allowed"
    }
  ]
}
